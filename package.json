    "cookie-session": "^2.0.0-beta.3",
    "debug": "^4.1.0",
    "del": "^3.0.0",
    "dnode": "^1.2.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-flash": "^0.0.2",
    "express-formidable": "^1.0.0",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "formidable": "^1.2.1",
    "fs-readfile-promise": "^3.0.1",
