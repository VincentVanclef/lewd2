  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "start:dev": "nodemon dist/app.js",
    "watch": "node_modules/.bin/concurrently \"npm:watch-server\" \"npm:watch-less\"",
    "build": "node_modules/.bin/concurrently \"npm:build-server\" \"npm:build-less\"",
    "build-server": "node_modules/.bin/parcel build src/app.js src/cronactions.js --target node",
    "build-less": "node_modules/.bin/parcel build Public/CSS/main.less --out-dir Public/CSS",
    "watch-server": "node_modules/.bin/parcel src/app.js src/cronactions.js --target node",
    "watch-less": "node_modules/.bin/parcel watch Public/CSS/main.less --out-dir Public/CSS"
  },
